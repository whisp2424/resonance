{
    "name": "resonance",
    "version": "0.1.0",
    "main": "dist/main/index.js",
    "author": {
        "name": "whisp2424",
        "email": "hello@whisp.moe",
        "url": "https://whisp.moe"
    },
    "lint-staged": {
        "*.{html,css,js,ts,jsx,tsx,json,md}": [
            "prettier --write"
        ],
        "*.{js,ts,jsx,tsx}": [
            "eslint --fix --max-warnings=0"
        ]
    },
    "scripts": {
        "prepare": "husky",
        "rebuild": "electron-rebuild -f",
        "postinstall": "electron-builder install-app-deps",
        "dev": "electron-vite dev",
        "start": "electron-vite preview",
        "build": "pnpm run check:node && electron-vite build",
        "build:win": "pnpm run build && electron-builder --win",
        "build:linux": "pnpm run build && electron-builder --linux",
        "build:unpack": "pnpm run build && electron-builder --dir",
        "check:node": "tsc --noEmit -p tsconfig.node.json --composite false",
        "check:web": "tsc --noEmit -p tsconfig.web.json --composite false",
        "check": "pnpm run check:node && pnpm run check:web",
        "lint": "eslint --cache .",
        "lint:fix": "eslint --cache --fix .",
        "format": "prettier --write .",
        "fix": "pnpm run lint:fix && pnpm run format",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:push": "drizzle-kit push"
    },
    "dependencies": {
        "@electron-toolkit/preload": "^3.0.2",
        "@electron-toolkit/utils": "^4.0.0",
        "@libsql/client": "^0.17.0",
        "@tanstack/react-hotkeys": "^0.1.0",
        "@tanstack/react-query": "^5.90.21",
        "arktype": "^2.1.29",
        "chokidar": "^5.0.0",
        "picocolors": "^1.1.1",
        "react-hook-form": "^7.71.1",
        "write-file-atomic": "^7.0.0",
        "zustand": "^5.0.11"
    },
    "devDependencies": {
        "@base-ui/react": "^1.2.0",
        "@electron-toolkit/eslint-config-prettier": "^3.0.0",
        "@electron-toolkit/eslint-config-ts": "^3.1.0",
        "@electron-toolkit/tsconfig": "^2.0.0",
        "@electron-toolkit/typed-ipc": "^1.0.2",
        "@electron/rebuild": "^4.0.3",
        "@eslint/js": "^10.0.1",
        "@iconify-json/fluent": "^1.2.38",
        "@iconify-json/lucide": "^1.2.90",
        "@svgr/core": "^8.1.0",
        "@svgr/plugin-jsx": "^8.1.0",
        "@tailwindcss/vite": "^4.1.18",
        "@types/node": "^25.2.3",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "@types/write-file-atomic": "^4.0.3",
        "@vitejs/plugin-react": "^5.1.4",
        "clsx": "^2.1.1",
        "drizzle-kit": "^0.31.9",
        "drizzle-orm": "^0.45.1",
        "electron": "^40.4.1",
        "electron-builder": "^26.7.0",
        "electron-vite": "^5.0.0",
        "eslint": "^10.0.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.5.0",
        "globals": "^17.1.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "prettier": "^3.8.1",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "react-router-dom": "^7.13.0",
        "tailwind-merge": "^3.4.0",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.55.0",
        "unplugin-icons": "^23.0.1",
        "vite": "^7.3.1",
        "vite-plugin-svgr": "^4.5.0"
    }
}
